// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
}

model Project {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String
  githubId    Int
  imageUrl    String
}
